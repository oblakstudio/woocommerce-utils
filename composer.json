{
  "name": "oblak/woocommerce-utils",
  "description": "This module contains various utilities for WooCommerce.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Sibin Grasic",
      "email": "sibin.grasic@oblak.studio"
    }
  ],
  "require": {
    "php": ">= 8.0",
    "oblak/wp-polyfills": "^1.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.31",
    "oblak/wordpress-coding-standard": "^1.0"
  },
  "suggest": {
    "automattic/jetpack-autoloader": "Allow for better interoperability with other plugins that use this package."
  },
  "autoload": {
    "psr-4": {
      "Oblak\\WooCommerce\\": "src",
      "\\": "src/Interfaces/Shared"
    },
    "files": [
      "src/Utils/woocommerce-utils-functions.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true
    }
  }
}
